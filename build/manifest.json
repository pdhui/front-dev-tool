{
  "name": "front-dev",
  "manifest_version":2,
  "version": "0.1.0",
  "description": "frontEnd assist tool 前端视觉还原辅助工具",
  "content_scripts": [ {
      "js": [ "main.js" ],
      "matches": [ "*://*/*", "file:///*" ]
   } ],
